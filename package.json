{
  "name": "rh-homepage",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tailwind-start": "npx tailwindcss -i ./style.css -o ./dist/style.css --watch",
    "tailwind-build": "npx tailwindcss -i ./style.css -o ./dist/style.css --minify",
    "serve": "npx @11ty/eleventy --serve",
    "start": "npx parallelshell \"npm run tailwind-start\" \"npm run serve\"",
    "clean-dist": "npx rimraf dist",
    "build": "npm run clean-dist && npx @11ty/eleventy && npm run tailwind-build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "parallelshell": "^3.0.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.7"
  },
  "dependencies": {
    "swup": "^2.0.14"
  }
}
